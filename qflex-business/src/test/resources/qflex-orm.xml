	<!--QUERY DECLARATIONS FOR CURRENT DATASOURCE-->
	<QUERY NAME="qmanagerStatsByDatetime">
		<![CDATA[ 
		SELECT q.queue_id, q.queue_nm, qs.fetch_start_tm, qs.fetch_end_tm,  qs.msg_in, qs.msg_out, (qs.msg_in + qs.msg_out) throughput 
		FROM queue_stats qs, queues q  
		WHERE qs.queue_id = q.queue_id
		AND q.qmanager_id = ?
		AND (qs.fetch_start_tm >= ? AND qs.fetch_end_tm <= ?)
		order by q.queue_id,q.queue_nm, qs.fetch_start_tm, qs.fetch_end_tm
		]]>
	</QUERY>
	
	<QUERY NAME="qmanagerStatsByInterval">
		<![CDATA[ 
		SELECT q.queue_id, q.queue_nm, qs.fetch_start_tm, qs.fetch_end_tm, qs.msg_in, qs.msg_out, (qs.msg_in + qs.msg_out) throughput 
		FROM queue_stats qs, queues q  
		WHERE qs.queue_id = q.queue_id
		AND q.qmanager_id = ?
		AND ( (? BETWEEN qs.fetch_start_tm AND qs.fetch_end_tm) OR qs.fetch_start_tm >= ?)
		order by q.queue_id,q.queue_nm, qs.fetch_start_tm, qs.fetch_end_tm
		]]>
	</QUERY>
	
	<QUERY NAME="qmanagerStatsSummaryByDatetime">
		<![CDATA[ 
		SELECT q.queue_id, q.queue_nm, sum(qs.msg_out) msg_out, sum(qs.msg_in) msg_in, max(qs.high_depth) high_depth
		FROM queue_stats qs, queues q  
		WHERE qs.queue_id = q.queue_id
		AND q.qmanager_id = ?
		AND (qs.fetch_start_tm >= ? AND qs.fetch_end_tm <= ?) {0}
		group by q.queue_id, q.queue_nm
		order by q.queue_nm, q.queue_id
		]]>
	</QUERY>
	
	<QUERY NAME="qmanagerStatsSummaryByInterval">
		<![CDATA[ 
		SELECT q.queue_id, q.queue_nm, sum(qs.msg_out) msg_out, sum(qs.msg_in) msg_in, max(qs.high_depth) high_depth
		FROM queue_stats qs, queues q  
		WHERE qs.queue_id = q.queue_id
		AND q.qmanager_id = ?
		AND ( (? BETWEEN qs.fetch_start_tm AND qs.fetch_end_tm) OR qs.fetch_start_tm >= ?) {0}
		group by q.queue_id, q.queue_nm
		order by q.queue_nm, q.queue_id
		]]>
	</QUERY>
	
	<QUERY NAME="qmanagersStatsSummaryByDatetime">
		<![CDATA[ 
		SELECT qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias, sum(qs.msg_out) msg_out, sum(qs.msg_in) msg_in, max(qs.high_depth) high_depth
		FROM queue_stats qs, qmanagers qm, queues q  
		WHERE qs.queue_id = q.queue_id
		AND q.qmanager_id = qm.qmanager_id
		AND qm.company_id = ?
		AND (qs.fetch_start_tm >= ? AND qs.fetch_end_tm <= ?) {0}
		group by qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias
		order by qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias
		]]>
	</QUERY>
	
	<QUERY NAME="qmanagersStatsSummaryByInterval">
		<![CDATA[ 
		SELECT qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias, sum(qs.msg_out) msg_out, sum(qs.msg_in) msg_in, max(qs.high_depth) high_depth
		FROM queue_stats qs, qmanagers qm, queues q  
		WHERE qs.queue_id = q.queue_id
		AND q.qmanager_id = qm.qmanager_id
		AND qm.company_id = ?
		AND ( (? BETWEEN qs.fetch_start_tm AND qs.fetch_end_tm) OR qs.fetch_start_tm >= ?) {0}
		group by qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias
		order by qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias
		]]>
	</QUERY>
	
	<QUERY NAME="qstatsToBeCompresed">
		<![CDATA[ 
		SELECT qs.* FROM queue_stats qs, queues q  
		WHERE qs.queue_id = q.queue_id
		AND q.qmanager_id = ?
		AND ( (qs.fetch_start_tm >= ?) AND ((? - qs.fetch_start_tm) > ?))
		order by q.qmanager_id, q.queue_id, qs.fetch_start_tm, qs.fetch_end_tm
		]]>
	</QUERY>
	
	<QUERY NAME="qmanagersThroughputSummary">
		<![CDATA[ 
		SELECT qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias, (sum(qs.msg_in) + sum(qs.msg_out)) throughput 
		FROM queue_stats qs, queues q, qmanagers qm
		WHERE qs.queue_id = q.queue_id
		AND q.qmanager_id = qm.qmanager_id
		AND qm.company_id = ?
		AND ( (? BETWEEN qs.fetch_start_tm AND qs.fetch_end_tm) OR qs.fetch_start_tm >= ?)
		GROUP BY qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias
		ORDER BY throughput desc
		]]>
	</QUERY>
	
	<QUERY NAME="qmanagerThroughputSummary">
		<![CDATA[ 
		SELECT qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias, (sum(qs.msg_in) + sum(qs.msg_out)) throughput 
		FROM queue_stats qs, queues q, qmanagers qm
		WHERE qs.queue_id = q.queue_id
		AND q.qmanager_id = qm.qmanager_id
		AND qm.qmanager_id = ?
		AND ( (? BETWEEN qs.fetch_start_tm AND qs.fetch_end_tm) OR qs.fetch_start_tm >= ?)
		GROUP BY qm.qmanager_id, qm.qmanager_nm, qm.qmanager_alias
		ORDER BY throughput desc
		]]>
	</QUERY>
	
	<QUERY NAME="deleteAllZeroQueueStats">
		<![CDATA[ 
		DELETE FROM queue_stats
		WHERE (msg_in = 0 AND msg_out = 0 AND high_depth = 0)
		AND ((? - fetch_start_tm) > ?)
		]]>
	</QUERY>
	
	<QUERY NAME="nodeStatsSummaryForFlowOnPeriod">
		<![CDATA[ 
		SELECT ns.wmbi_node_id, n.node_nm, sum(ns.cpu_tm) total_cpu_time, sum(ns.elapsed_tm) total_elapsed_time, sum(ns.invocations) total_invocations
		FROM wmbi_node_stats ns, wmbi_nodes n, wmbi_snapshots ss
		WHERE ss.wmbi_snapshot_id = ns.wmbi_snapshot_id
		AND n.wmbi_node_id = ns.wmbi_node_id
		AND n.wmbi_flow_id = ?
		AND (ss.start_tm >= ? AND ss.end_tm <= ?)
		GROUP BY n.wmbi_node_id, n.node_nm
		ORDER BY n.wmbi_node_id, n.node_nm
		]]>
	</QUERY>
	
	<QUERY NAME="nodeStatsOnPeriod">
		<![CDATA[ 
		SELECT ns.wmbi_node_id, n.node_nm, ns.cpu_tm, ns.min_cpu_tm, ns.max_cpu_tm
		, ns.elapsed_tm, ns.min_elapsed_tm, ns.max_elapsed_tm, ss.start_tm, ss.end_tm
		FROM wmbi_node_stats ns, wmbi_nodes n, wmbi_snapshots ss
		WHERE ss.wmbi_snapshot_id = ns.wmbi_snapshot_id
		AND n.wmbi_node_id = ?
		AND (ss.start_tm >= ? AND ss.end_tm <= ?)
		ORDER BY n.wmbi_node_id, n.node_nm, ss.start_tm, ss.end_tm
		]]>
	</QUERY>
	
	<QUERY NAME="flowStatsSummaryForBrokerOnPeriod">
		<![CDATA[
		SELECT f.wmbi_flow_id, eg.exec_group_nm, f.flow_nm, sum(ts.cpu_tm) total_cpu_time, sum(ts.in_msgs) total_input_messages, sum(ts.in_msgs_size) total_size_of_input_messages
		FROM wmbi_thread_stats ts, wmbi_flows f, wmbi_exec_groups eg, wmbi_snapshots ss
		WHERE ss.wmbi_snapshot_id = ts.wmbi_snapshot_id
		AND f.wmbi_exec_group_id = eg.wmbi_exec_group_id
		AND f.wmbi_flow_id = ts.wmbi_flow_id
		AND ss.wmbi_broker_id = ?
		AND (ss.start_tm >= ? AND ss.end_tm <= ?)
		GROUP BY eg.exec_group_nm, f.wmbi_flow_id, f.flow_nm
		ORDER BY eg.exec_group_nm, f.wmbi_flow_id, f.flow_nm
		]]>
	</QUERY>
	
	<QUERY NAME="flowStatsOnPeriod">
		<![CDATA[
		SELECT f.wmbi_flow_id, f.flow_nm, ts.cpu_tm, ts.in_msgs, ts.in_msgs_size, ts.elapsed_tm, ss.start_tm, ss.end_tm
		FROM wmbi_thread_stats ts, wmbi_flows f, wmbi_snapshots ss
		WHERE ss.wmbi_snapshot_id = ts.wmbi_snapshot_id
		AND f.wmbi_flow_id = ts.wmbi_flow_id
		AND ts.wmbi_flow_id = ?
		AND (ss.start_tm >= ? AND ss.end_tm <= ?)
		ORDER BY f.wmbi_flow_id, f.flow_nm, ss.start_tm, ss.end_tm
		]]>
	</QUERY>
	
	<QUERY NAME="snapshotsToBeCompressed">
		<![CDATA[
		SELECT ss.* FROM wmbi_snapshots ss
		WHERE ss.wmbi_broker_id = ? AND (ss.start_tm >= ? AND ss.end_tm < ?)
		ORDER BY ss.wmbi_broker_id, ss.start_tm, ss.end_tm
		]]>
	</QUERY>
